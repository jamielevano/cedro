            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion04") FUNCIONARIOS DE GOB. LOCALES
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion05") LIDERES/DIRIGENTES COMUNALES
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion06") ADMINISTRADORES DE NEGOCIOS
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion07") EMPRENDEDORES
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion08") MICROEMPRESARIOS
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion09") MUJERES FACILITADORAS
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion10") PERIODISTAS/COMUNICADORES
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion11") DOCENTE
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion12") ESTUDIANTES
            <td>
        </tr>
        <tr>
            <td>
                @Html.koCheck("poblacion13") LIDERES DE OPINION
