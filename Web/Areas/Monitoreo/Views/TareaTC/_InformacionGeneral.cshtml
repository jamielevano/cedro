<table style="width: 100%;">
    @*<colgroup>
        <col style="width:30%"/>
        <col />
    </colgroup>*@
    <tr>
        @*<td>
        </td>*@
        <td style="text-align:right">
            <button type="button"  data-bind="click: function(){FichaArchivo(0)}"><a class="btn folder"></a> Agregar Archivo</button>
        </td>
    </tr>
</table>
<table style="width: 75%; float:left;border: 1px solid navy">
    <colgroup>
        <col style="width:20%"/>
        <col style="width:30%"/>
        <col style="width:20%"/>
        <col style="width:30%"/>
    </colgroup>
    <tr>
        <td>EJE DE INTERVENCION</td>
        <td>@Html.koSelect("ejeintervencionid", "$root.listEjeIntervencion")</td>
        <td>LOCALIDAD</td>
        <td>@Html.koSelect("telecentroid", "$root.listTelecentros", "($root.listTelecentros() && !$root.bloqueo)")</td>
    </tr>
    <tr>
        <td>SECTOR</td>
        <td>@Html.koSelect("sectorid", "$root.listSector")</td>
        <td>REGION</td>
        <td>@Html.koSelect("regionid", "$root.listRegion")</td>
    </tr>
    <tr>
        <td>FECHA INICIO</td>
        <td>@Html.koDate("fechainicio")</td>
        <td>FECHA FIN</td>
        <td>@Html.koDate("fechafin", min: "fechainicio")</td>
    </tr>
    <tr>
        <td>FECHA DE LLENADO FICHA GENERAL</td>
        <td>@Html.koDate("fechareporte", min: "fechainicio")</td>
        <td>PROMOTOR RESPONSABLE</td>
        <td>@Html.koSelect("promotorid", "$root.listUsuario", enable: "false")</td>
    </tr>
    
    <tr>
        @*<td>N° de Productores</td>
        <td>@Html.koNumeric("productores", @decimal: false,maxlength:3)</td>*@
        @*<td>N° de No Productores</td>
        <td>
            <table>
                <tr>
                    <td>
                        @Html.koNumeric("noproductores", @decimal: false,maxlength:3)
                    </td>
                    <td><input type="text" readonly="readonly" title="Productores y no productores total" style="text-align: right;width:40px" data-bind="value: +productores() + +noproductores()" /></td>
                </tr>
            </table>
            @Html.koChkNumeric("((+productores() + +noproductores()) == (+participanteshombres() + +participantesmujeres())) && ( (+productores() + +noproductores()) == (+rangoedad1() + +rangoedad2() + +rangoedad3()))")
        </td>*@
    </tr>
    
    <tr>
        <td>DESCRIPCION DE LA ACTIVIDAD</td>
        <td colspan="2"><textarea name="descripcion" data-bind="value: descripcion" rows="3" data-val="true"></textarea></td>
    </tr>
</table>


<div>    
    <table  style="width: 25%; float:right;border: 1px solid navy">
        <tr>
            <td>
                POBLACION PARTICIPANTE
