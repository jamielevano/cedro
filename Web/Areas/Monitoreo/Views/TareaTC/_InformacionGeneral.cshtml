            <td>
                @*@Html.koCheck("poblacion15") Mujeres<br />
                @Html.koCheck("poblacion16") Discapacitados<br />*@
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;OTROS @Html.koTextNoRequired("poblacionotro", maxlength: 200)<br /><br /><br /><br />
                @Html.koChkVal("poblacion01() || poblacion02() || poblacion03() || poblacion04() || poblacion05() || poblacion06() || poblacion07() || poblacion08() || poblacion09() || poblacion10() || poblacion11() || poblacion12() || poblacion13() || poblacion14() || poblacionotro()")
            </td>
        </tr>
    </table>    
   
</div>

<div > 
        
    <table style="width: 75%; float:left;border: 1px solid navy">
        <tr>
            <td>INFORMACION SUSTENTATORIA</td>
        </tr>
        <tr>
            <td>@Html.koCheck("sustentoficha1") FICHA 1</td>
