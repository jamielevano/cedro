<div >
    <table style="width: 75%; float:left;border: 1px solid navy">
        <tr>
            <td>PARTICIPANTES HOMBRES</td>
            <td>@Html.koNumeric("participanteshombres", @decimal: false, maxlength: 5, required: false)</td>
            <td>PARTICIPANTES MUJERES</td>
            <td>
                <table>
                    <tr>
                        <td>
                            @Html.koNumeric("participantesmujeres", @decimal: false, maxlength: 5, required: false)                            
                        </td>
                        <td>TOTAL :</td>
                        <td><input type="text" readonly="readonly" title="Participantes total" style="text-align: right;width:40px" data-bind="value: +participanteshombres() + +participantesmujeres()" /></td>
                    </tr>
                </table>
                @Html.koChkNumeric("((+productores() + +noproductores()) == (+participanteshombres() + +participantesmujeres())) && ( (+productores() + +noproductores()) == (+rangoedad2() + +rangoedad3() + +rangoedad4()))")
               
            </td>
        </tr>
    </table>
</div>
<div>
    <table style="width: 75%; float:left;border: 1px solid navy">
      
        <tr>
            <td colspan="4">
                <table style="width: 80%;">
                    <colgroup>
                        <col />
                        <col style="width:20%" />
                        <col />
                        <col style="width:20%" />
                        <col />
                        <col style="width:20%" />
                        <col />
                    </colgroup>
                    <tr>
                        <td colspan="6" style="align-content:center">RANGO DE EDAD</td>
                    </tr>
                    <tr>
                        <td>12-17</td>
                        <td>@Html.koNumeric("rangoedad2", @decimal: false, maxlength: 3, required: false)</td>
                        <td>18-30</td>
                        <td>@Html.koNumeric("rangoedad3", @decimal: false, maxlength: 3, required: false)</td>
                        <td>31 a más</td>
                        <td>@Html.koNumeric("rangoedad4", @decimal: false, maxlength: 3, required: false)</td>
                        <td>TOTAL :</td>
                        <td>
                            <input type="text" readonly="readonly" title="Rangos de edad total" style="text-align: right;width:40px" data-bind="value: +rangoedad2() + +rangoedad3() + +rangoedad4()" />
                        </td>
                    </tr>
                </table>
                @Html.koChkNumeric("((+productores() + +noproductores()) == (+participanteshombres() + +participantesmujeres())) && ( (+productores() + +noproductores()) == (+rangoedad2() + +rangoedad3() + +rangoedad4()))")
            </td>
        </tr>
      
    </table>
</div>
