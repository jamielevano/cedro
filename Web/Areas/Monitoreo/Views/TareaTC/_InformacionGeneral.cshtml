    @Html.koCheck("sustentoficha3") ficha3<br />
    @Html.koCheck("sustentoficha4") ficha4<br />
    @Html.koCheck("sustentoficha5") ficha5<br />
    @Html.koCheck("sustentoficha6") ficha6<br />
    @Html.koCheck("sustentovideos") Otros [videos/fotos]<br />
    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Especificar @Html.koTextNoRequired("sustentootros",maxlength:200)<br />
    @Html.koChkVal("sustentoficha1() || sustentoficha2() || sustentoficha3() || sustentoficha4() || sustentoficha5() || sustentoficha6() || sustentovideos() || sustentootros()")*@
